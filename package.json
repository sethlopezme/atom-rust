{
  "name": "atom-rust",
  "main": "./lib/atom-rust",
  "version": "0.2.0",
  "description": "Provide Rust language support in Atom.",
  "keywords": [
    "rust",
    "language",
    "server"
  ],
  "repository": "https://github.com/sethlopezme/atom-rust",
  "license": "MIT",
  "engines": {
    "atom": ">=1.17.0 <2.0.0"
  },
  "dependencies": {
    "atom-languageclient": "git://github.com/atom/atom-languageclient.git"
  },
  "consumedServices": {
    "linter-indie": {
      "versions": {
        "2.0.0": "consumeLinterV2"
      }
    },
    "status-bar": {
      "versions": {
        "^1.0.0": "consumeStatusBar"
      }
    }
  },
  "providedServices": {
    "autocomplete.provider": {
      "versions": {
        "2.0.0": "provideAutocomplete"
      }
    },
    "hyperclick.provider": {
      "versions": {
        "0.0.0": "provideHyperclick"
      }
    }
  },
  "configSchema": {
    "command": {
      "title": "RLS Command",
      "description": "Command to start the Rust Language Server.",
      "type": "string",
      "default": "rustup run nightly rls"
    },
    "useRlsRoot": {
      "title": "Use RLS_ROOT",
      "description": "Use the value of the `RLS_ROOT` environment variable as the current working directory when starting RLS.",
      "type": "boolean",
      "default": false
    }
  }
}
